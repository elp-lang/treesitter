{
  "grammars": [
    {
      "name": "elp",
      "camelcase": "Elp",
      "scope": "source.elp",
      "path": ".",
      "file-types": [
        ".elp",
        ".velp",
        ".selp"
      ],
      "injection-regex": "^elp$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "GNU",
    "description": "Treesitter code for the Elp programming language",
    "authors": [
      {
        "name": "davemackintosh",
        "email": "me@dav3.co"
      }
    ],
    "links": {
      "repository": "https://github.com/elp-lang/treesitter"
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "node": true,
    "python": false,
    "rust": false,
    "swift": false
  }
}
